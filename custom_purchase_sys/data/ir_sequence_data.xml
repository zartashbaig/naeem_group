<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">


                        <!-- Sequences for Inquiry -->
        <record id="Inquiry_purchase_order_ext" model="ir.sequence">
            <field name="name">Purchase</field>
            <field name="code">cyb.purchase</field>
            <field name="prefix">PI</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

<!--         Sequences for Quotation -->
        <record id="quotation_2_sale_order_ext" model="ir.sequence">
            <field name="name">Quotation</field>
            <field name="code">cyb.quotation.purchase</field>
            <field name="prefix">Qu</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>



    </data>
</odoo>
